@prefix gpii: <http://registry.gpii.org/>.
@prefix prefs: <http://registry.gpii.org/preference/>.
@prefix apps: <http://registry.gpii.org/applications/>.
@prefix ax: <http://gpii.org/schemas/accessibility#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

[InferRequiredAT:
(?x prefs:name "magnifierEnabled")
->
(ax:User ax:requiresAT "Magnifier")
]
[InferRequiredAT:
(?x prefs:name "screenReaderTTSEnabled")
->
(ax:User ax:requiresAT "Screen Reader")
]
[InferRequiredAT:
(?x prefs:name ?y)
(?y apps:type "Screen Reader")
->
(ax:User ax:requiresAT "Screen Reader")
]
[InferRequiredAT:
(?x prefs:name ?y)
(?y apps:type "Magnifier")
->
(ax:User ax:requiresAT "Magnifier")
]

[InferPreferedAT:
(?x gpii:preference ?y)
(?y prefs:type "applications")
->
(ax:User ax:prefersAT ?y)
]